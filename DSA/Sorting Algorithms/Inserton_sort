def insertion_sort(arr):
    key = arr[0]
    n = len(arr)
    for i in range(1,n):
        key = arr[i]
        j = i-1
        
    while j >=0 and arr[j] >key:
        arr[j+1] = arr[j]
        j -=1
        
    arr[j+1] = key 
    return arr
    
arr = [1, 2, 3, 4, 8, 44, 67, 123]
print(insertion_sort(arr))
