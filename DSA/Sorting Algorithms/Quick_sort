def quick_sort(arr):
    if len(arr) <= 1:
        return arr
        
    pivot = arr[-1]
    left = []
    right = []
    
    for num in arr[:-1]:
        if num <= pivot:
            left.append(num)
        else:
            right.append(num)
    return  quick_sort(left) +[pivot]+ quick_sort(right)
    
    
    
arr = [34,6,1,7,9,45,23]
print(quick_sort(arr))
